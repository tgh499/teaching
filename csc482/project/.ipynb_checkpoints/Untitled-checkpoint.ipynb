    "def encrypt(text):\n",
    "    char_list = []\n",
    "    for i in text:\n",
    "        char_list.append(ord(i)+6)\n",
    "\n",
    "    text = \"\"\n",
    "    for i in char_list:\n",
    "        text += chr(i)\n",
    "    print(text)\n",
    "\n",
